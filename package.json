{
  "name": "llm-notes-cron",
  "version": "1.0.0",
  "description": "A simple TypeScript application",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "NODE_ENV=development tsx src/index.ts",
    "dev:pretty": "NODE_ENV=development LOG_LEVEL=debug tsx src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "test": "node --test --loader=tsx/esm src/**/*.test.ts",
    "test:watch": "node --test --watch --loader=tsx/esm src/**/*.test.ts",
    "test:coverage": "node --test --experimental-test-coverage --loader=tsx/esm src/**/*.test.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "depcheck": "depcheck",
    "docker:build": "docker build -t llm-notes-cron .",
    "docker:run": "docker run --rm llm-notes-cron",
    "docker:run:interactive": "docker run --rm -it llm-notes-cron",
    "docker:dev": "docker build -t llm-notes-cron . && docker run --rm llm-notes-cron"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.11.1",
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^2.0.2",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0"
  }
}
